<div id="<%= dom_id employee %>">
  <p>
    <strong>Name:</strong>
    <%= employee.name %>
  </p>

  <p>
    <strong>Age:</strong>
    <%= employee.age %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= employee.email %>
  </p>

  <p>
    <strong>Street:</strong>
    <%= employee.street %>
  </p>

  <p>
    <strong>House number:</strong>
    <%= employee.house_number %>
  </p>

  <p>
    <strong>Neighborhood:</strong>
    <%= employee.neighborhood %>
  </p>

  <p>
    <strong>City:</strong>
    <%= employee.city %>
  </p>

  <p>
    <strong>State:</strong>
    <%= employee.state %>
  </p>

</div>
